  pod 'Firebase/Firestore'
  pod 'FirebaseFirestoreSwift'

  pod 'M13Checkbox'
  pod 'OverlayContainer'

  pod 'SwiftLint', '0.39.2'
  pod 'PromiseKit', '6.13.1'

end
